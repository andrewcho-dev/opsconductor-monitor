 --
 -- Interface IPv6 Addresses
 --
 wwpLeosIpInterfaceIPv6AddressTable OBJECT-TYPE
     SYNTAX SEQUENCE OF WwpLeosIpInterfaceIPv6AddressEntry
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
             "Table of IPv6 addresses and subnet masks associated to an IP Interface."
     ::= { wwpLeosIpInterface 7 }

 wwpLeosIpInterfaceIPv6AddressEntry OBJECT-TYPE
     SYNTAX     WwpLeosIpInterfaceIPv6AddressEntry
     MAX-ACCESS not-accessible
     STATUS     current
     DESCRIPTION
             "IPv6 address and subnet mask associated with an IP Interface."
     INDEX { wwpLeosIpDataInterfaceIndex, wwpLeosIpInterfaceIPv6AddressIndex }
     ::= { wwpLeosIpInterfaceIPv6AddressTable 1 }

 WwpLeosIpInterfaceIPv6AddressEntry ::=  SEQUENCE { 
      wwpLeosIpInterfaceIPv6AddressIndex      Integer32,
      wwpLeosIpInterfaceIpInetAddrType        InetAddressType,
      wwpLeosIpInterfaceIpInetAddr            InetAddress,
      wwpLeosIpInterfaceSubnetInetAddrType    InetAddressType,
      wwpLeosIpInterfaceSubnetInetAddr        InetAddress
 }

 wwpLeosIpInterfaceIPv6AddressIndex OBJECT-TYPE
     SYNTAX       Integer32 (1..4096)
     MAX-ACCESS   not-accessible
     STATUS       current
     DESCRIPTION
             "Index into the table."
     ::= { wwpLeosIpInterfaceIPv6AddressEntry 1 }

 wwpLeosIpInterfaceIpInetAddrType OBJECT-TYPE
     SYNTAX       InetAddressType
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
        "The type of address wwpLeosIpInterfaceIpInetAddr belongs to.
         When set to:
         ipv4 : wwpLeosIpInterfaceIpInetAddr should be compliant with 
                InetAddressIPv4 from RFC 4001
         ipv6 : wwpLeosIpInterfaceIpInetAddr should be compliant with
                InetAddressIPv6 from RFC 4001."
     ::= { wwpLeosIpInterfaceIPv6AddressEntry 2 }

 wwpLeosIpInterfaceIpInetAddr OBJECT-TYPE
     SYNTAX       InetAddress
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
        "IP address associated with this interface. This address should be
         compliant with RFC 4001."
     ::= { wwpLeosIpInterfaceIPv6AddressEntry 3 }

 wwpLeosIpInterfaceSubnetInetAddrType OBJECT-TYPE
     SYNTAX       InetAddressType
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
        "Specifies the type of address wwpLeosIpInterfaceSubnetInetAddr belongs to.
         When set to:
         ipv4 : wwpLeosIpInterfaceSubnetInetAddr should be compliant with 
                InetAddressIPv4 from RFC 4001
         ipv6 : wwpLeosIpInterfaceSubnetInetAddr should be compliant with
                InetAddressIPv6 from RFC 4001."
     ::= { wwpLeosIpInterfaceIPv6AddressEntry 4 }

 wwpLeosIpInterfaceSubnetInetAddr OBJECT-TYPE
     SYNTAX       InetAddress
     MAX-ACCESS   read-only
     STATUS       current
     DESCRIPTION
        "The subnet mask associated with the ip address/interface. 
         This address should be compliant with RFC 4001."
     ::= { wwpLeosIpInterfaceIPv6AddressEntry 5 }

 wwpLeosIpInterfaceIPv6AddrAddedNotification NOTIFICATION-TYPE
     OBJECTS {
                  cienaGlobalSeverity,
                  cienaGlobalMacAddress,
                  wwpLeosIpDataInterfaceName,
                  wwpLeosIpInterfaceIpInetAddrType,
                  wwpLeosIpInterfaceIpInetAddr,
