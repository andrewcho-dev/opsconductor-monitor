	-- 1.3.6.1.2.1.16.2.3.1.23
	tokenRingMLHistoryFrequencyErrors OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The total number of frequency errors reported in
