	-- 1.3.6.1.2.1.16.2.4.1.16
	tokenRingPHistoryDataPkts4096to8191Octets OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The total number of good non-MAC frames received
