 cienaCesBgpLspMIBObjects OBJECT IDENTIFIER ::= { cienaCesBgpLspMIB 1 }
 
 -- Notifications   
 cienaCesBgpLspMIBNotificationPrefix  OBJECT IDENTIFIER ::=  { cienaCesNotifications 103 } 
 cienaCesBgpLspMIBNotifications       OBJECT IDENTIFIER ::=  { cienaCesBgpLspMIBNotificationPrefix 0 } 
 --
 -- BGP LSP Node
