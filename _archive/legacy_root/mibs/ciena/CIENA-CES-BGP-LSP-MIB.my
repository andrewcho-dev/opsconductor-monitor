--
 -- Textual convention
 --
 NHLFEType ::= TEXTUAL-CONVENTION
     STATUS       current
     DESCRIPTION  "NHLFE Type"
     SYNTAX       INTEGER {
                    ipv4AdjType(1),
                    rsvpTunlType(2),
                    none(3),
                    ipv6Adj(4),
                    unnumbered(5)
                   }

 TunnelType ::= TEXTUAL-CONVENTION
     STATUS       current
     DESCRIPTION  "Tunnel Type"
     SYNTAX       INTEGER {
                    ingress(1),
                    egress(2),
                    transit(3)
                   }

            
 --
 -- Node definitions
