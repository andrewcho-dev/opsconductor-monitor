 --
 -- BGP Peer State change Traps
 --

 cienaCesBgpPeerInEstState NOTIFICATION-TYPE
    OBJECTS {
        cienaGlobalSeverity,  
        cienaGlobalMacAddress,
        bgpPeerLastError,
        bgpPeerRemoteAddr,
        bgpPeerState
    }
    STATUS  current
    DESCRIPTION
         "This notification is generated when BGP peer enters into Established State."
    ::= { cienaCesBgpLspMIBNotifications  1 }


 cienaCesBgpPeerOutEstState NOTIFICATION-TYPE
    OBJECTS {
        cienaGlobalSeverity,  
        cienaGlobalMacAddress,
        bgpPeerLastError,
        bgpPeerRemoteAddr,
        bgpPeerState
    }
    STATUS  current
    DESCRIPTION
         "This notification is generated when BGP peer moves out of Established State."

    ::= { cienaCesBgpLspMIBNotifications 2 }

